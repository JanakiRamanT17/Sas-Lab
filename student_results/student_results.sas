DATA RESULT;
    SET RESULT;
    LENGTH Result $30;
    IF Marks >= 90 THEN Result = "Outstanding";
    ELSE IF Marks >= 75 THEN Result = "First Class with Distinction";
    ELSE IF Marks >= 65 THEN Result = "First Class";
    ELSE IF Marks >= 50 THEN Result = "Second Class";
    ELSE Result = "Fail";
RUN;

TITLE "RESULT";
PROC PRINT DATA=RESULT;
RUN;

TITLE "FINAL RESULT";
PROC REPORT DATA=RESULT NOWD;
    COLUMN USN Name Gender Marks Result;
    DEFINE Result / DISPLAY STYLE=[FOREGROUND=blue];
RUN;
